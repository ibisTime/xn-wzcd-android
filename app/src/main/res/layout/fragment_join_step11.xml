<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <com.cdkj.wzcd.view.MyEditLayout
                android:id="@+id/myEl_110_contact_name1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_input_hint="请输入联系人姓名"
                app:txt_input_title="*联系人1姓名" />

            <com.cdkj.wzcd.view.MySelectLayout
                android:id="@+id/mySl_110_relationship1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_select_title="*与申请人关系" />

            <com.cdkj.wzcd.view.MyEditLayout
                android:id="@+id/myEl_110_contact_number1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:inputType="phone"
                app:txt_input_hint="请输入手机号码"
                app:txt_input_title="*手机号码" />

            <com.cdkj.wzcd.view.MyEditLayout
                android:id="@+id/myEl_110_contact_name2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_input_hint="请输入联系人姓名"
                app:txt_input_title="联系人2姓名" />

            <com.cdkj.wzcd.view.MySelectLayout
                android:id="@+id/mySl_110_relationship2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_select_title="与申请人关系" />

            <com.cdkj.wzcd.view.MyEditLayout
                android:id="@+id/myEl_110_contact_number2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:inputType="phone"
                app:txt_input_hint="请输入手机号码"
                app:txt_input_title="手机号码" />


        </LinearLayout>

    </ScrollView>
</layout>

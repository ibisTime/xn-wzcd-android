<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.cdkj.wzcd.module.cartool.gps.GpsListActivity">

        <com.cdkj.wzcd.view.MyNormalLayout
            android:id="@+id/myNl_status"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:txt_normal_title="状态">

        </com.cdkj.wzcd.view.MyNormalLayout>

        <com.cdkj.wzcd.view.MyNormalLayout
            android:id="@+id/myNl_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:txt_normal_title="申请个数">

        </com.cdkj.wzcd.view.MyNormalLayout>

        <com.cdkj.wzcd.view.MyNormalLayout
            android:id="@+id/myNl_reason"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:txt_normal_title="申请说明">

        </com.cdkj.wzcd.view.MyNormalLayout>

        <View style="@style/style_split_line_horizontal_broad" />

        <com.cdkj.wzcd.view.MyNormalLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:txt_normal_title="GPS列表">

        </com.cdkj.wzcd.view.MyNormalLayout>

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recycler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


        </android.support.v7.widget.RecyclerView>
    </LinearLayout>
</layout>

<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <LinearLayout
        style="@style/style_my_item_root">

        <com.cdkj.wzcd.view.item.MyItemDoubleLayout
            android:id="@+id/myItemDl_periods_and_repayCapital"
